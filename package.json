{
  "name": "doppio",
  "version": "0.0.0",
  "description": "doppio tracking",
  "main": "index.js",
  "scripts": {
    "test": "tape tests/*",
    "dev": "npm run watch:css & npm run preview",
    "preview": "DEBUG=$([ ! -z \"$DEBUG\" ] && echo \"$DEBUG\" || echo \"rekkarit\") NODE_ENV=preview npm run preview:server",
    "preview:server": "./node_modules/.bin/beefy --cwd public src/js/main.js:bundle.js 8080 -- -d -t babelify -t envify",
    "build": "DEBUG=\"\" NODE_ENV=production npm run build:all",
    "build:all": "npm run build:css && npm run build:js && npm run build:manifest && npm run build:index",
    "build:js": "./node_modules/.bin/browserify src/js/main.js -t babelify -t envify > public/bundle.js",
    "build:css": "./node_modules/.bin/postcss -u cssnext -o public/style.css src/css/global.css",
    "build:manifest": "echo build manifest",
    "build:index": "echo build index",
    "watch:css": "./node_modules/.bin/cssnext -w src/css/global.css public/style.css"
  },
  "keywords": [
    "espresso",
    "doppio",
    "coffee"
  ],
  "author": "Niklas Lindgren <nikc@iki.fi>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.4.7",
    "babelify": "^6.1.2",
    "beefy": "^2.1.5",
    "browserify": "^10.2.1",
    "cssnext": "^1.5.1",
    "envify": "^3.4.0",
    "postcss-cli": "^1.3.1",
    "tape": "^4.0.0"
  },
  "dependencies": {
    "alt": "^0.16.6",
    "debug": "^2.2.0",
    "react": "^0.13.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikcorg/doppio.git"
  },
  "homepage": "http://doppio.kakspistenolla.com"
}
